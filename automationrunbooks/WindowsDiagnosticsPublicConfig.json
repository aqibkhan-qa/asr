{
  "WadCfg": {
    "DiagnosticMonitorConfiguration": {
      "Metrics": { "MetricAggregation": [] },
      "DiagnosticInfrastructureLogs": {},
      "Directories": {
        "IISLogs": { "containerName": "wad-iis-logfiles" },
        "FailedRequestLogs": { "containerName": "wad-failedrequestlogs" },
        "scheduledTransferPeriod": "PT1M"
      },
      "PerformanceCounters": {
        "PerformanceCounterConfiguration": [
          {
            "annotation": [],
            "counterSpecifier": "\\Processor(_Total)\\% Processor Time",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Memory\\Available MBytes",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Memory\\Committed Bytes",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Memory\\Page Faults/sec",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\LogicalDisk(_Total)\\Disk Read Bytes/sec",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\LogicalDisk(C:)\\Disk Write Bytes/sec",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\LogicalDisk(C:)\\Disk Read Bytes/sec",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Web Service(_Total)\\ISAPI Extension Requests/sec",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Web Service(_Total)\\Bytes Total/Sec",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\ASP.NET Applications(__Total__)\\Requests/Sec",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\ASP.NET Applications(__Total__)\\Errors Total/Sec",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\ASP.NET\\Requests Queued",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\ASP.NET\\Requests Rejected",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\ASP.NET\\Applications Running",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(WaWorkerHost)\\% Processor Time",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(WaWorkerHost)\\Private Bytes",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(WaWorkerHost)\\Thread Count",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(WaWebHost)\\% Processor Time",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(WaWebHost)\\Private Bytes",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(WaWebHost)\\Thread Count",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(WaIISHost)\\% Processor Time",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(WaIISHost)\\Private Bytes",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(WaIISHost)\\Thread Count",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(IISExpress)\\% Processor Time",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(IISExpress)\\Private Bytes",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(IISExpress)\\Thread Count",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(w3wp)\\% Processor Time",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(w3wp)\\Private Bytes",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\Process(w3wp)\\Thread Count",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\.NET CLR Interop(_Global_)\\# of marshalling",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\.NET CLR Jit(_Global_)\\% Time in Jit",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\.NET CLR Loading(_Global_)\\% Time Loading",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\.NET CLR LocksAndThreads(_Global_)\\Contention Rate / sec",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\.NET CLR Memory(_Global_)\\# Bytes in all Heaps",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\.NET CLR Memory(_Global_)\\% Time in GC",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\.NET CLR Networking(_Global_)\\Connections Established",
            "sampleRate": "PT3M"
          },
          {
            "annotation": [],
            "counterSpecifier": "\\.NET CLR Remoting(_Global_)\\Remote Calls/sec",
            "sampleRate": "PT3M"
          }
        ],
        "scheduledTransferPeriod": "PT1M"
      },
      "WindowsEventLog": {
        "DataSource": [
          { "name": "System!*[System[(Level=1 or Level=2)]]" },
          { "name": "Security!*[System[(Level=1 or Level=2)]]" },
          { "name": "Application!*[System[(Level=1 or Level=2 or Level=3)]]" },
          { "name": "Windows Azure!*[System[(Level=1 or Level=2 or Level=3 or Level=4)]]" }
        ],
        "scheduledTransferPeriod": "PT1M"
      },
      "CrashDumps": {
        "CrashDumpConfiguration": [
          { "processName": "WaAppAgent.exe" },
          { "processName": "WaIISHost.exe" },
          { "processName": "WindowsAzureGuestAgent.exe" },
          { "processName": "WaWorkerHost.exe" },
          { "processName": "DiagnosticsAgent.exe" },
          { "processName": "w3wp.exe" }
        ],
        "DumpType": "Full"
      },
      "Logs": { "scheduledTransferPeriod": "PT1M" },
      "overallQuotaInMB": 4096
    }
  }
}